{
  "name": "minimal-react-webpack-babel-setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "startDevServer": "rimraf dist && webpack-dev-server --config ./webpack.config.js --mode development",
    "symlinks": "npm link dmn-llqhelpers dmnengine docxgen",
    "buildProd": "rimraf dist && npm run symlinks && webpack --mode production --config ./webpack.prod.js ",
    "buildTest": "rimraf dist && npm run symlinks && webpack --mode development --config ./webpack.test.js --bail --colors --display-error-details --json > WebpackBuildStats.json",
    "TStype-check": "tsc --emitDeclarationOnly",
    "TStype-check:watch": "npm run type-check -- --watch",
    "test": "echo \"No test specified\" && exit 0"
  },
